{
  "manifest_version": 2,
  "name": "Wiki-o'-My-Thumb",
  "version": "0.1",

  "content_scripts": [
    {
      "matches": [
        "*://*.wikipedia.org/*"
      ],
      "js": ["jquery-3.1.1.min.js", "content.js"]
    }
  ],

  "background": {
    "scripts": ["FileSaver.min.js", "background.js"]
  }
}
